.custom-combobox
  position relative
  display inline-block

.custom-combobox-toggle
  position absolute
  top 0
  bottom 0
  margin-left -1px
  padding 0

.custom-combobox-input
  margin 0
  padding 5px 10px

.col-main
  padding-top: 11px

.page-header
  margin-bottom: 40px
  margin-top: 0
  border: 0

.sidebar-right
  position fixed
  width: 26%
  top 51px
  bottom 0
  right 0
  padding: 30px 30px
  z-index 1000
  display block
  // padding 20px
  overflow-x hidden
  overflow-y auto
  background-color #f5f5f5
  border-left 1px solid #eee

.sidebar-buttons
  width: 100%
  .btn
    width: 50%

.page-header .fa-stack
  margin-left: 10px
  font-size: 14px
  vertical-align: -3px
